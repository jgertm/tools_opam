    name = "dynlink",
    version = "[distributed with OCaml]",
    archive = select({
        "@ocaml//mode:bytecode": [":dynlink.cma"],
        "@ocaml//mode:native"  : [
            ":dynlink.cmxa",
            ":dynlink.a"
